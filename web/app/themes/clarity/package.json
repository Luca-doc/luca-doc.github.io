{
    "name": "clarity",
    "version": "2.1.0",
    "engines": {
        "node": "11.10.0",
        "npm": "6.7.0"
    },
    "description": "Laravel Mix compiler for Clarity theme",
    "main": "index.js",
    "scripts": {
        "dev": "NODE_ENV=development webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "NODE_ENV=development webpack --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "hot": "NODE_ENV=development webpack-dev-server --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "production": "NODE_ENV=production webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ministryofjustice/intranet.git"
    },
    "contributors": [
        "MoJ Digital team"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ministryofjustice/intranet/issues"
    },
    "homepage": "https://github.com/ministryofjustice/intranet/#readme",
    "dependencies": {
        "autoprefixer": "^9.7.6",
        "chalk": "^2.4.2",
        "ctgen": "^1.3.2",
        "del": "^3.0.0",
        "jquery": "^3.5.0",
        "node-notifier": "^5.4.3"
    },
    "devDependencies": {
        "@babel/core": "^7.8.7",
        "@babel/preset-env": "^7.9.5",
        "copy-webpack-plugin": "^5.1.1",
        "cssnano": "^4.1.10",
        "imagemin-webpack-plugin": "^2.4.2",
        "jeet": "^7.2.0",
        "laravel-mix": "^5.0.4",
        "laravel-mix-imagemin": "^1.0.3",
        "pretty": "^2.0.0",
        "resolve-url-loader": "^2.3.1",
        "rupture": "^0.7.1",
        "stylus": "^0.54.7",
        "stylus-loader": "^3.0.2",
        "vue-template-compiler": "^2.6.11"
    },
    "author": "",
    "standard": {
        "globals": [
            "jQuery",
            "myAjax"
        ]
    },
    "browserslist": [
        "last 4 version",
        "> 5%",
        "maintained node versions",
        "not dead",
        "not ie <= 8"
    ]
}
