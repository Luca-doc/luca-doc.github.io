@charset "UTF-8";

// Please note, there is a nifty little mixin that uses these icons in mixin.styl
// which means you don't need to add a class.

@font-face {
  font-family: "moji-clarity";
  src:url("../fonts/moji-clarity.eot");
  src:url("../fonts/moji-clarity.eot?#iefix") format("embedded-opentype"),
    url("../fonts/moji-clarity.woff") format("woff"),
    url("../fonts/moji-clarity.ttf") format("truetype"),
    url("../fonts/moji-clarity.svg#moji-clarity") format("svg");
  font-weight: normal;
  font-style: normal;

}

[data-icon]:before {
  font-family: "moji-clarity" !important;
  content: attr(data-icon);
  font-style: normal !important;
  font-weight: normal !important;
  font-variant: normal !important;
  text-transform: none !important;
  speak: none;
  line-height: 1;

}

[class^="icon-"]:before,
[class*=" icon-"]:before {
  font-family: "moji-clarity" !important;
  font-style: normal !important;
  font-weight: normal !important;
  font-variant: normal !important;
  text-transform: none !important;
  speak: none;
  line-height: 1;
}

